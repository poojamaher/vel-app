pipeline {
  agent any
  
   stages {
   stage ('compile-code')  {
    steps {
	    sh 'mvn clean compile'
		}
	}
		
		stage ('test-code')  {
		steps {
		   sh 'mvn test'
		 }
    }
		   
		   stage ('deploy-code') {
		   steps {
		      sh ' mvn install'
	             }
	          }
			  
        	}
         }
